meta {
  name: Get Volunteer By ID
  type: http
  seq: 2
}

get {
  url: {{user_service_url}}/api/v1/users/volunteers/{{volunteer_id}}
  body: none
  auth: inherit
}

tests {
  test("Should retrieve volunteer by ID", function() {
    expect(res.status).to.equal(200);
    expect(res.body).to.have.property("id");
    expect(res.body).to.have.property("email");
    expect(res.body.role).to.equal("VOLUNTEER");
  });
}