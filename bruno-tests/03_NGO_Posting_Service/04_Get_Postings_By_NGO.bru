meta {
  name: Get Postings By NGO
  type: http
  seq: 4
}

get {
  url: {{ngo_service_url}}/api/v1/postings/ngo/{{ngo_id}}?page=0&size=20
  body: none
  auth: bearer
  token: {{ngo_token}}
}

params:query {
  page: 0
  size: 20
}

tests {
  test("Should retrieve NGO's postings", function() {
    expect(res.status).to.equal(200);
    expect(res.body).to.have.property("content");
    expect(res.body.content).to.be.an("array");
  });
}